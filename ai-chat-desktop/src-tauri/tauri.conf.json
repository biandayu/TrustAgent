{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:8080"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  },
  "productName": "TrustAgent Desktop",
  "mainBinaryName": "TrustAgent Desktop",
  "version": "0.0.1",
  "identifier": "com.trustagent.aichat",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "TrustAgent Desktop",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "center": true,
        "decorations": true,
        "transparent": false,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}